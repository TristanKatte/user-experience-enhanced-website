<%- include('./partials/head') %>

<main>
    <section>
        <div id="popup" class="hidden">
            <p>Pop-up message!</p>
          </div>
        <form action="/vragenlijst" method="post">
            <% sdgs.forEach(function (sdg){ %>
                <label>
                    <input type="checkbox" name="id" value="<%=sdg.id%>">
                    <img src="https://fdnd-agency.directus.app/assets/<%=sdg.icon%> alt=" class="clickable-image">
                </label>
            <% }) %>
            <button type="submit" id="back-button" class="vragenlijst-btn">Naar vragenlijst</button>
        </form>
    </section>
</main>

    
<%- include('./partials/foot') %>